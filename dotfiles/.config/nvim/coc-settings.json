{
  "coc.preferences.extensionUpdateCheck": "never",
  "coc.source.emoji.filetypes": ["markdown", "rst"],
  "coc.source.dictionary.filetypes": [
    "gitcommit",
    "html",
    "markdown",
    "rst",
    "text"
  ],
  "coc.source.word.filetypes": ["gitcommit", "html", "markdown", "rst", "text"],
  "snippets.priority": 100000,
  "snippets.ultisnips.enable": false,
  "suggest.autoTrigger": "always",
  "suggest.noselect": true,
  "diagnostic.enable": true,
  "pullDiagnostic.onSave": true,
  "jedi.enable": false,
  "pyright.enable": true,
  "pyright.inlayHints.variableTypes": false,
  "pyright.inlayHints.functionReturnTypes": false,
  "signature.hideOnTextChange": true,
  "signature.enable": false,
  "sumneko-lua.enableNvimLuaDev": true,
  "Lua.workspace.checkThirdParty": true,
  "Lua.workspace.library": ["${3rd}/luv/library"],
  "Lua.hint.enable": false,
  "html.autoClosingTags": false,
  "html.completion.attributeDefaultValue": "empty",
  "vimlsp.suggest.fromRuntimepath": true,
  "vimlsp.indexes.count": 50,
  "java.enabled": false,
  "list.normalMappings": {
    "<C-v>": "action:vsplit",
    "<C-x>": "action:split",
    "<C-t>": "action:tabe"
  },
  "list.insertMappings": {
    "<C-v>": "action:vsplit",
    "<C-x>": "action:split",
    "<C-t>": "action:tabe"
  },
  "diagnostic-languageserver.filetypes": {
    "nginx": "nginx-linter"
  },
  "ltex.dictionary": {
    "en-US": [
      "DAG",
      "isort",
      "docformatter",
      "Neovim",
      "coc",
      "nvim",
      "DAGs",
      "Dotfiles",
      "EKS",
      "Filetype",
      "PyCQA",
      "Roeca",
      "dotfiles",
      "filetype",
      "ingestions"
    ]
  },
  "diagnostic-languageserver.linters": {
    "nginx-linter": {
      "sourceName": "nginx-linter",
      "command": "nginx-linter",
      "debounce": 100,
      "args": ["--include", "%file"],
      "formatPattern": [
        "^\\s+(\\d+?):(\\d+?)\\s+([a-z]+?)\\s+(.*)$",
        {
          "line": 1,
          "column": 2,
          "security": 3,
          "message": 4
        }
      ],
      "rootPatterns": ["nginx.conf", ".git"],
      "securities": {
        "error": "info"
      }
    }
  },
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "filetypes": ["haskell", "lhaskell"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ]
    },
    "nginx-language-server": {
      "command": "nginx-language-server",
      "filetypes": ["nginx"],
      "rootPatterns": ["nginx.conf", ".git"]
    },
    "terraform-ls": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "rootPatterns": ["main.tf", "Makefile", ".git"]
    },
    "tflint": {
      "command": "tflint",
      "args": ["--langserver"],
      "filetypes": ["terraform", "tf"],
      "rootPatterns": ["main.tf", "Makefile", ".git"]
    },
    "svelteserver": {
      "command": "svelteserver",
      "args": ["--stdio"],
      "filetypes": ["svelte"],
      "rootPatterns": ["package.json", ".git"]
    },
    "yaml": {
      "command": "yaml-language-server",
      "args": ["--stdio"],
      "filetypes": ["yaml"],
      "settings": {
        "yaml": {
          "schemas": {
            "kubernetes": ["/kubernetes/**"],
            "https://raw.githubusercontent.com/docker/compose/master/compose/config/compose_spec.json": "/*docker-compose.yml",
            "https://raw.githubusercontent.com/threadheap/serverless-ide-vscode/master/packages/serverless-framework-schema/schema.json": "/*serverless.yml",
            "https://raw.githubusercontent.com/OAI/OpenAPI-Specification/3.0.3/schemas/v3.0/schema.json": [
              "/*open-api*.yml",
              "/*open-api*.yaml"
            ]
          }
        }
      },
      "rootPatterns": [".git"]
    }
  },
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  }
}
