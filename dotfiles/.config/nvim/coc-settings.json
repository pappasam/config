{
  "coc.preferences.extensionUpdateCheck": "never",
  "coc.source.emoji.filetypes": ["markdown", "rst"],
  "coc.source.dictionary.filetypes": ["markdown", "rst"],
  "coc.source.word.filetypes": ["markdown", "rst"],
  "suggest.autoTrigger": "always",
  "suggest.noselect": true,
  "diagnostic.enable": true,
  "signature.hideOnTextChange": true,
  "signature.enable": false,
  "jedi.markupKindPreferred": "plaintext",
  "jedi.jediSettings.autoImportModules": ["numpy", "alembic"],
  "jedi.diagnostics.enable": false,
  "vimlsp.suggest.fromRuntimepath": true,
  "vimlsp.indexes.count": 50,
  "java.enabled": false,
  "list.normalMappings": {
    "<C-v>": "action:vsplit",
    "<C-x>": "action:split",
    "<C-t>": "action:tabe"
  },
  "list.insertMappings": {
    "<C-v>": "action:vsplit",
    "<C-x>": "action:split",
    "<C-t>": "action:tabe"
  },
  "pairs.enableCharacters": ["(", "[", "{", "<", "\"", "'"],
  "yaml.schemas": {
    "https://raw.githubusercontent.com/instrumenta/kubernetes-json-schema/master/v1.17.0-standalone-strict/all.json": [
      "/kubernetes/*.yml",
      "/kubernetes/*.yaml"
    ],
    "https://raw.githubusercontent.com/docker/compose/master/compose/config/config_schema_v3.8.json": "/*docker-compose.yml",
    "https://raw.githubusercontent.com/threadheap/serverless-ide-vscode/master/packages/serverless-framework-schema/schema.json": "/*serverless.yml",
    "https://raw.githubusercontent.com/OAI/OpenAPI-Specification/3.0.3/schemas/v3.0/schema.json": [
      "/*open-api*.yml",
      "/*open-api*.yaml"
    ]
  },
  "diagnostic-languageserver.filetypes": {
    "python": "pylint",
    "markdown": "write-good",
    "rst": "write-good"
  },
  "diagnostic-languageserver.formatFiletypes": {
    "python": ["black", "isort", "docformatter"]
  },
  "diagnostic-languageserver.linters": {
    "pylint": {
      "sourceName": "pylint",
      "command": "pylint",
      "debounce": 100,
      "args": [
        "--output-format",
        "text",
        "--score",
        "no",
        "--msg-template",
        "'{line}:{column}:{category}:{msg} ({msg_id}:{symbol})'",
        "%file"
      ],
      "formatPattern": [
        "^(\\d+?):(\\d+?):([a-z]+?):(.*)$",
        {
          "line": 1,
          "column": 2,
          "security": 3,
          "message": 4
        }
      ],
      "rootPatterns": ["pyproject.toml", "setup.py", ".git"],
      "securities": {
        "informational": "hint",
        "refactor": "info",
        "convention": "info",
        "warning": "warning",
        "error": "error",
        "fatal": "error"
      },
      "offsetColumn": 1,
      "formatLines": 1
    },
    "write-good": {
      "sourceName": "write-good",
      "command": "write-good",
      "debounce": 100,
      "args": ["--text=%text"],
      "offsetLine": 0,
      "offsetColumn": 1,
      "formatLines": 1,
      "formatPattern": [
        "(.*)\\s+on\\s+line\\s+(\\d+)\\s+at\\s+column\\s+(\\d+)\\s*$",
        {
          "line": 2,
          "column": 3,
          "message": 1
        }
      ]
    }
  },
  "diagnostic-languageserver.formatters": {
    "black": {
      "command": "black",
      "args": ["black", "-q", "-"]
    },
    "isort": {
      "command": "isort",
      "args": ["-q", "-"]
    },
    "docformatter": {
      "command": "docformatter",
      "args": ["-"]
    }
  },
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh", "bash", "zsh", "make"],
      "ignoredRootPaths": ["~"]
    },
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["Dockerfile"],
      "args": ["--stdio"]
    },
    "terraform-lsp": {
      "command": "terraform-lsp",
      "filetypes": ["terraform"],
      "rootPatterns": ["Makefile", ".git"]
    },
    "tflint": {
      "command": "tflint",
      "filetypes": ["terraform"],
      "rootPatterns": ["Makefile", ".git"],
      "args": ["--langserver"]
    },
    "cobol": {
      "command": "cobol-language-server",
      "filetypes": ["cobol"],
      "initializationOptions": {}
    }
  },
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  }
}
