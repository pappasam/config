{
  "coc.preferences.extensionUpdateCheck": "never",
  "coc.source.emoji.filetypes": ["markdown", "rst"],
  "coc.source.dictionary.filetypes": [
    "gitcommit",
    "html",
    "markdown",
    "rst",
    "text"
  ],
  "coc.source.word.filetypes": ["gitcommit", "html", "markdown", "rst", "text"],
  "suggest.autoTrigger": "always",
  "suggest.noselect": true,
  "diagnostic.enable": true,
  "signature.hideOnTextChange": true,
  "signature.enable": false,
  "jedi.jediSettings.autoImportModules": [
    "alembic",
    "fastai",
    "fastcore",
    "matplotlib",
    "sqlalchemy",
    "sympy",
    "django",
    "django.db"
  ],
  "jedi.startupMessage": false,
  "html.autoClosingTags": false,
  "html.completion.attributeDefaultValue": "empty",
  "svelte.enable-ts-plugin": true,
  "vimlsp.suggest.fromRuntimepath": true,
  "vimlsp.indexes.count": 50,
  "java.enabled": false,
  "rust-client.rlsPath": "rls",
  "rust-client.channel": "stable",
  "list.normalMappings": {
    "<C-v>": "action:vsplit",
    "<C-x>": "action:split",
    "<C-t>": "action:tabe"
  },
  "list.insertMappings": {
    "<C-v>": "action:vsplit",
    "<C-x>": "action:split",
    "<C-t>": "action:tabe"
  },
  "yaml.schemas": {
    "https://raw.githubusercontent.com/yannh/kubernetes-json-schema/master/v1.24.3-standalone-strict/all.json": [
      "/*.yml",
      "/*.yaml"
    ],
    "https://raw.githubusercontent.com/docker/compose/master/compose/config/compose_spec.json": "/*docker-compose.yml",
    "https://raw.githubusercontent.com/threadheap/serverless-ide-vscode/master/packages/serverless-framework-schema/schema.json": "/*serverless.yml",
    "https://raw.githubusercontent.com/OAI/OpenAPI-Specification/3.0.3/schemas/v3.0/schema.json": [
      "/*open-api*.yml",
      "/*open-api*.yaml"
    ]
  },
  "diagnostic-languageserver.filetypes": {
    "python": ["mypy", "pylint"],
    "nginx": "nginx-linter"
  },
  "diagnostic-languageserver.linters": {
    "nginx-linter": {
      "sourceName": "nginx-linter",
      "command": "nginx-linter",
      "debounce": 100,
      "args": ["--include", "%file"],
      "formatPattern": [
        "^\\s+(\\d+?):(\\d+?)\\s+([a-z]+?)\\s+(.*)$",
        {
          "line": 1,
          "column": 2,
          "security": 3,
          "message": 4
        }
      ],
      "rootPatterns": ["nginx.conf", ".git"],
      "securities": {
        "error": "info"
      }
    },
    "mypy": {
      "sourceName": "mypy",
      "command": "mypy",
      "args": [
        "--no-color-output",
        "--no-error-summary",
        "--show-column-numbers",
        "--follow-imports=silent",
        "%file"
      ],
      "formatPattern": [
        "^.*:(\\d+?):(\\d+?): ([a-z]+?): (.*)$",
        {
          "line": 1,
          "column": 2,
          "security": 3,
          "message": 4
        }
      ],
      "securities": {
        "error": "error"
      },
      "offsetColumnEnd": 1,
      "rootPatterns": ["mypy.ini", "pyproject.toml", "setup.py", ".git"]
    },
    "pylint": {
      "sourceName": "pylint",
      "command": "pylint",
      "debounce": 100,
      "args": [
        "--output-format",
        "text",
        "--score",
        "no",
        "--msg-template",
        "'{line}:{end_line}:{column}:{end_column}:{category}:{msg} ({msg_id}:{symbol})'",
        "%file"
      ],
      "formatPattern": [
        "^(\\d+?):(\\d+?):(\\d+?):(\\d+?):([a-z]+?):(.*)$",
        {
          "line": 1,
          "endLine": 2,
          "column": 3,
          "endColumn": 4,
          "security": 5,
          "message": 6
        }
      ],
      "rootPatterns": ["pyproject.toml", "setup.py", ".git"],
      "securities": {
        "informational": "hint",
        "refactor": "info",
        "convention": "info",
        "warning": "warning",
        "error": "error",
        "fatal": "error"
      },
      "offsetColumn": 1,
      "formatLines": 1
    }
  },
  "languageserver": {
    "efm": {
      "command": "efm-langserver",
      "args": [],
      "filetypes": ["markdown"],
      "initializationOptions": {
        "documentFormatting": false,
        "hover": true,
        "documentSymbol": false,
        "codeAction": false,
        "completion": false
      }
    },
    "terraform-ls": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "rootPatterns": ["Makefile", ".git"]
    },
    "tflint": {
      "command": "tflint",
      "args": ["--langserver"],
      "filetypes": ["terraform", "tf"],
      "rootPatterns": ["Makefile", ".git"]
    }
  },
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  }
}
